# ============================================
# Google Vertex AI (Gemini) Configuration
# ============================================
# For AI-Assisted Scheduling explanation layer
#
# Authentication: Uses Application Default Credentials (ADC)
# - Locally: Run `gcloud auth application-default login`
# - Cloud Run: Assign service account with Vertex AI User role
#
VERTEX_AI_ENABLED=false
VERTEX_AI_PROJECT_ID=connected-capacity-21
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL=gemini-1.5-pro

# Generation parameters (optional)
VERTEX_AI_TEMPERATURE=0.3
VERTEX_AI_MAX_OUTPUT_TOKENS=512
VERTEX_AI_TOP_P=0.8
VERTEX_AI_TOP_K=40

# Safety settings (optional)
VERTEX_AI_SAFETY_THRESHOLD=BLOCK_MEDIUM_AND_ABOVE

# Timeouts and rate limiting (optional)
VERTEX_AI_TIMEOUT_SECONDS=30
VERTEX_AI_MAX_RETRIES=2
VERTEX_AI_RATE_LIMIT_RPM=60

# Logging (optional)
VERTEX_AI_LOGGING_ENABLED=true
VERTEX_AI_LOG_CHANNEL=stack
